int main() {
	double a1,a2,a3,sum;
	cout <<"Enter the angles of a triangle\n";
	cin >>a1>>a2>>a3;
	sum = a1+a2+a3;
	if (sum=180)
	{
		if (a1==a2 && a2==a3 && a3==a1)
		cout <<"The triangle is equilateral.";
		else
		if (a1==a2 && a1!=a3 ||a2==a3 && a2!=a1 ||a3==a1 && a3!=a2)
		cout <<"The triangle is isoceles.";
		else
		cout <<"The triangle is scalene.";
	}
	else
	cout <<"Invalid entry.";
	return 0;
}
