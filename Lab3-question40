#include <iostream>
#include <cmath>
using namespace std;

int main() {
	int n,dig,temp,div,num;
	cout <<"Enter a number\n";
	cin >>n;
	temp = n;
	dig = 0;
	while(temp>0)
	{
		temp = temp/10;
		dig = dig + 1;
	}
	while(n>0)
	{
		dig = dig - 1;
		div = pow(10,dig);
		num = n/div;
		n = n%div;
		if (num == 0)
		cout <<"Zero ";
		else
		if (num == 1)
		cout <<"One ";
		else
		if (num == 2)
		cout <<"Two ";
		else
		if (num == 3)
		cout <<"Three ";
		else
		if (num == 4)
		cout <<"Four ";
		else
		if (num==5)
		cout <<"Five ";
		else
		if (num == 6)
		cout <<"Six ";
		else
		if (num == 7)
		cout <<"Seven ";
		else
		if (num == 8)
		cout <<"Eight ";
		else
		if (num == 9)
		cout <<"Nine ";
		
		
		
		
	}
	
	return 0;
}
