#include <iostream>
#include <cmath>
using namespace std;

int main() {
	
	int n,num,i,sum,dig,cube;
	sum = 0;
	cout <<"Enter a number\n";
	cin >>n;
	cout <<"List of Armstrong numbers is : \n";
	for(i=1; i<=n; i++)
	{
		num = i;
		while(num>1)
	{
		dig = num%10;
		cube = pow(dig,3);
		sum = sum+cube;
		num = num/10;
	}
	if (sum == i)
	cout <<i <<endl;
	}
	return 0;
}
